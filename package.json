{
    "scripts": {
        "dfx_install": "DFX_VERSION=0.15.1 sh -ci \"$(curl -fsSL https://sdk.dfinity.org/install.sh)\"",
        "replica_start": "dfx start --background",
        "replica_stop": "dfx stop",
        "canister_deploy_local": "dfx deploy uploads",
        "canister_deploy_mainnet": "dfx deploy --network ic uploads",
        "canister_uninstall": "dfx canister uninstall-code uploads",
        "canister_call_get_message": "dfx canister call uploads getMessage",
        "canister_call_set_message": "dfx canister call uploads setMessage '(\"Hello world!\")'"
    },
    "dependencies": {
        "18": "^0.0.0",
        "azle": "0.18.6"
    }
}
